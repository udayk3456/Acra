function fixedLineValidation(evt) {
    var source = evt.getSource();
    var moibleId = source.getProperty("lineField");
    var mobileNumber;
    var msgId = source.getProperty("lineMsgField");
    var msg = source.findComponent(msgId);

    if (moibleId == null) {
        mobileNumber = source.getValue();
    }
    else {
        mobileNumber = source.findComponent(moibleId).getValue();
    }
    if ((mobileNumber.length == 8) && ((mobileNumber.indexOf(6) == 0) || (mobileNumber.indexOf(3) == 0))) {
        msg.setMessage("");
        AdfPage.PAGE.addPartialTrigger(msg);
        return true;

    }
    else {
        evt.cancel();
        msg.setMessage("Mobile number is invalid!");
        AdfPage.PAGE.addPartialTrigger(msg);
        return null;
    }
}

