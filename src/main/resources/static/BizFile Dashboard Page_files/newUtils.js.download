isShift = false

var oldShareValue;

function oldShareValue(evt) {
    var source = evt.getSource();
    var _keyCode = evt.getKeyCode();
    var clientId = source.getClientId();
    var unitposition;
    var shareStr1;
    var shareStr2;
    clientId = clientId + '::content';
    var shareField = document.getElementById(clientId);
    var shareNo = shareField.value;
    var resultV;
    var regex = new RegExp("^(\\d{0,19}(\\.\\d{0,19})?|\\d{0,19})$");
    //Backspace, Right and Left Arrow
    if (_keyCode == 8 || _keyCode == 37 || _keyCode == 39) {
        return true;
    }
    //For Decimal
    if (_keyCode == 110 || _keyCode == 190) {
        if (shareNo != null) {
            unitposition = document.getElementById(evt.getSource().getClientId() + "::content").selectionStart;
            console.log(unitposition);
            shareStr1 = shareNo.substr(0, unitposition);
            shareStr2 = shareNo.substr(unitposition);
            shareNo = shareStr1 + String.fromCharCode(46) + shareStr2;
        }
        else {
            shareNo = String.fromCharCode(46);
        }

        //console.log("Part 1 :::" + shareNo);
        resultV = regex.test(shareNo);
        if (resultV) {
            //console.log(resultV);
            return true;
        }
        else {
            //console.log("Else");
            evt.cancel();
        }
    }
    else {
        if (_keyCode == 16) {
            isShift = true;
        }
        if (isShift == true || ((_keyCode == 32) || (_keyCode > 64 && _keyCode < 91) || (_keyCode > 105 && _keyCode < 112) || (_keyCode > 185 && _keyCode < 193) || (_keyCode > 218 && _keyCode < 223))) {
            //console.log("Not Valid");
            evt.cancel();
            return false;
        }
        if (shareNo != null) {
            unitposition = document.getElementById(evt.getSource().getClientId() + "::content").selectionStart;
            //console.log(unitposition);
            shareStr1 = shareNo.substr(0, unitposition);
            shareStr2 = shareNo.substr(unitposition);
            shareNo = shareStr1 + String.fromCharCode((96 <= _keyCode && _keyCode <= 105) ? _keyCode - 48 : _keyCode) + shareStr2;
        }
        else
            shareNo = String.fromCharCode((96 <= _keyCode && _keyCode <= 105) ? _keyCode - 48 : _keyCode);

        //console.log("Part 2 ::: "+shareNo);
        resultV = regex.test(shareNo);
        if (resultV) {
            //console.log(resultV);
            return true;
        }
        else {
            //console.log("Else "+resultV);
            evt.cancel();
        }
    }
}

