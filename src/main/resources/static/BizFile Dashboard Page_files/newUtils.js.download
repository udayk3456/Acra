    AdfPage.PAGE.clearMessages(comp.getClientId());
}

function setNaemFocus(evt) {
    var source = evt.getSource();
    var dateId = source.getProperty("focusField1");
    var id = source.findComponent(dateId).getValue();
    var field = source.getProperty("focusField");
    if (id == "1") {
        source.findComponent(field).focus();
    }
}

function characterCounter1(evt) {
    var source = evt.getSource();
    var outField1 = source.getProperty("outField");
    var maxlimit = parseInt(source.getProperty("maxLength"), 10);
    var outputLabelForMsg = source.findComponent(outField1);
    if (source.getValue() != 'null') {
        //alert('Characters Left :'+ (maxlimit - (source.getSubmittedValue().length)))
        if (source.getSubmittedValue().length > maxlimit) {
            alert('cross max limit')
            source.setValue(source.getSubmittedValue().substring(0, maxlimit));
        }
        else {
            outputLabelForMsg.setValue((maxlimit - (source.getSubmittedValue().length)));
        }
        AdfPage.PAGE.addPartialTrigger(source);
        AdfPage.PAGE.addPartialTrigger(outputLabelForMsg);
    }
}

