/*
     * Shift : 16
        ")" : 48
        "!" : 49
        "@" : 50
        "#" : 51
        "$" : 52
        "^" : 54
        "&" : 55
        "*" : 56
        "(" : 57
        ":" : 59
        "+" : 61
        "__" : 173
        "<" : 188
        ">" : 190
        "?" : 191
        "~" : 192
        "{" : 219
        "|" : 220
        "}" : 221


        ";" : 59
        "=" : 61
        "*" : 106
        "-" : 173
        ”,” : 188
        "." : 190
        "/" : 191
        "[" : 219
        "\" : 220
        "]" : 221
        "‘" : 222
     */
function validateProposedName(evt) {
    var _keyCode = evt.getKeyCode();
    console.log("_keyCode " + _keyCode);
    var _filterField = evt.getCurrentTarget();
    var _oldValue = _filterField.getValue();
    if (_keyCode == 16)
        isShift = true;//else shiftkeydn=false;
    if ((isShift == true && ((_keyCode > 47 && _keyCode < 53) || (_keyCode > 53 && _keyCode < 58) || (_keyCode > 189 && _keyCode < 193) || (_keyCode > 218 && _keyCode < 222) || _keyCode == 59 || _keyCode == 61 || _keyCode == 173 || _keyCode == 188)))
        return true;
    if ((isShift == true && _keyCode == 53)) {
        _filterField.setValue(_oldValue);
        evt.cancel();
    }
    keychar = String.fromCharCode(_keyCode);
}

function validateOfficerName(evt) {
    var _keyCode = evt.getKeyCode();

    var _filterField = evt.getCurrentTarget();
    var _oldValue = _filterField.getValue();
    if (_keyCode == 16)
        isShift = true;//else shiftkeydn=false;
    if ((isShift == true && ((_keyCode > 49 && _keyCode < 53) || (_keyCode > 53 && _keyCode < 58))) || (isShift == true && _keyCode == 48))
        return true;
    if ((isShift == true && _keyCode == 49) || (isShift == true && _keyCode == 53) || (_keyCode > 47 && _keyCode < 58) || (_keyCode > 95 && _keyCode < 106)) {
        _filterField.setValue(_oldValue);
        evt.cancel();
    }
    keychar = String.fromCharCode(_keyCode);

}

