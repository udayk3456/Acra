function futureDateValidationNew(evt) {
    var date = new Date();
    var curDate = date.getUTCDate();
    var curMnt = date.getMonth() + 1;
    var curYr = date.getUTCFullYear();
    var dateExp = /^(0?[1-9]|[12][0-9]|3[01])[\/\-](0?[1-9]|1[012])[\/\-]\d{4}$/;
    var source = evt.getSource();
    var dateId = source.getProperty("dateField");
    var id = source.findComponent(dateId);
    var msgId = source.getProperty("dateMsgField");
    var msg = source.findComponent(msgId);
    var field = source.getProperty("fieldName");
    if (id.getSubmittedValue() == "") {
        evt.cancel();
//        msg.setMessage("");
//        AdfPage.PAGE.addPartialTrigger(msg);
        return null;
    }
    if (!id.getSubmittedValue().match(dateExp)) {
        evt.cancel();
        if (field != null) {
            msg.setMessage("Please enter a valid " + field + "");
        }
        else {
            msg.setMessage("Please enter a valid  " + field + "");
        }
        AdfPage.PAGE.addPartialTrigger(msg);
        return null;
    }
    var dt = id.getSubmittedValue().substring(0, 2);
    var mnt = id.getSubmittedValue().substring(3, 5);
    var yr = id.getSubmittedValue().substring(6, 10);
    if (yr < 1900) {
        evt.cancel();
        if (field != null) {
            msg.setMessage("Please enter a valid " + field + ".");
        }
        else {
            msg.setMessage("Please enter a valid date."+ field + " must be greater than 1900");
        }
        AdfPage.PAGE.addPartialTrigger(msg);
        return null;
    }
    if (yr > curYr || (yr == curYr && mnt > curMnt) || (yr == curYr && mnt == curMnt && dt > curDate)) {
        evt.cancel();
        if (field != null) {
            msg.setMessage("Please enter a valid date. " + field + " must be an earlier date.");
        }
        else {
            msg.setMessage("Please enter a valid date."+ field + " must be a future date");
        }
        AdfPage.PAGE.addPartialTrigger(msg);
        return null;
    }
    msg.setMessage("");
    AdfPage.PAGE.addPartialTrigger(msg);
}

